{
  "name": "fhevm-universal-sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps - framework-agnostic, developer-friendly, and production-ready",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install && npm run bootstrap",
    "bootstrap": "npm install --workspaces",
    "build": "npm run build --workspaces",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs-example",
    "dev:react": "npm run dev --workspace=examples/react-example",
    "dev:agriculture": "npm run dev --workspace=examples/agriculture-insurance",
    "compile:contracts": "npx hardhat compile",
    "deploy:localhost": "npx hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "test": "npm run test --workspaces",
    "test:sdk": "npm run test --workspace=packages/fhevm-sdk",
    "lint": "npm run lint --workspaces",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "npm run clean --workspaces && npx hardhat clean"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.4",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fhevm/hardhat-plugin": "^0.0.1-3",
    "@fhevm/solidity": "^0.7.0",
    "@openzeppelin/contracts": "^5.4.0",
    "dotenv": "^17.2.3",
    "fhevm": "^0.6.0",
    "fhevmjs": "^0.5.0"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "blockchain",
    "sdk",
    "zama",
    "confidential-computing",
    "react",
    "nextjs",
    "vue",
    "framework-agnostic"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
